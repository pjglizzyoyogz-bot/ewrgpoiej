-- Scan all plots
local function scanAllPlots()
    local plots = workspace:FindFirstChild('Plots')
    if not plots then
        return
    end

    detectedBrainrots.highValue, detectedBrainrots.secretLowValue, detectedBrainrots.mediumValue =
        {}, {}, {}

    for _, plot in pairs(plots:GetChildren()) do
